{
    "manifest_version": 3,
    "name": "CV MANAGER ",
    "version": "1.7.1",
    "description": "Reconocer y autorrellenar formularios.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "hello.html",
        "default_icon": {
            "16": "imagenes/icon.png",
            "48": "imagenes/icon.png",
            "128": "imagenes/icon.png"
        }
    },
    
    "web_accessible_resources": [{
        
        "resources": ["crear_hoja_de_vida.html"], 
        "matches": ["<all_urls>"]
    }]
}
